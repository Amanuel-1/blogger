  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prepare": "node scripts/pre-build.js",
    "lint": "next lint --no-cache",
    "analyze": "cross-env ANALYZE=true next build",
    "build": "node scripts/pre-build.js && next build",
    "start": "next start",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md,mdx,scss,prisma,yaml}\"",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate-create-only": "prisma migrate dev --create-only",
    "prisma:generate": "prisma generate",
    "prisma:reset": "prisma migrate reset",
    "postinstall": "prisma generate"
  },
  "prettier": {
    "endOfLine": "lf",
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
